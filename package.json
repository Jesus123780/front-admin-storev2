{
  "name": "nextjs_approuter_electron",
  "version": "0.0.2",
  "private": true,
  "homepage": "https://github.com/Jesus123780/front-admin-storev2",
  "author": {
    "name": "venty",
    "email": "juvinaojesusd@gmail.com",
    "url": "https://github.com/Jesus123780/front-admin-storev2"
  },
  "main": "build/main.js",
  "description": "venty",
  "scripts": {
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "next:lint": "next lint",
    "format": "dprint fmt",
    "postinstall": "electron-builder install-app-deps",
    "electron:dist": "electron-builder --dir",
    "electron:dist:deb": "electron-builder --linux deb",
    "build": "run-s next:build electron:build",
    "electron:build": "electron-builder",
    "dist": "run-s build electron:dist",
    "prebuild:backend": "node electron/src/build-backend.js",
    "dev": "concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"next dev -p 3000\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build_watch": "tsup --watch",
    "electron:dev": "cross-env NODE_ENV='development' electron .",
    "electron:watch": "cross-env NODE_ENV='development' nodemon",
    "link:pkgch": "npm link npm-pkg-hook pkg-components rm -rf .next",
    "link:pkgche": "npm link npm-pkg-hook pkg-components eslint-config-next-front &&  rm -rf .next",
    "remove:ports:3000": "npx kill-port 3000",
    "remove:ports:8080": "npx kill-port 8080",
    "test": "npm run test:next && npm run test:electron",
    "test:next": "jest --config jest.config.ts",
    "test:electron": "jest --config jest.electron.config.ts",
    "test:watch": "npm run test -- --watch",
    "coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "npm run lint && npm run type-check",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.13.5",
    "@electron-toolkit/utils": "^3.0.0",
    "@electron/remote": "2.1.2",
    "@fingerprintjs/fingerprintjs": "^4.6.1",
    "@next/env": "^14.2.5",
    "apollo-upload-client": "^17.0.0",
    "clientjs": "^0.2.1",
    "cookie": "^1.0.2",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.5.0",
    "electron-log": "^5.3.4",
    "eslint-plugin-boundaries": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "express": "^5.1.0",
    "get-port-please": "^3.1.2",
    "google-auth-library": "9.15.1",
    "graphql-ws": "5.16.2",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "luxon": "3.6.1",
    "md5": "^2.3.0",
    "next": "^15.2.4",
    "npm-pkg-hook": "^1.11.6",
    "pg": "8.14.1",
    "pkg-components": "^2.2.3",
    "qr.js": "^0.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^5.3.11",
    "subscriptions-transport-ws": "^0.11.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@eslint/js": "^9.27.0",
    "@next/eslint-plugin-next": "^15.5.6",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.18.1",
    "babel-jest": "^30.2.0",
    "concurrently": "9.1.2",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "eslint": "^9.38.0",
    "eslint-config-next": "^14.2.33",
    "eslint-config-next-front": "1.0.8",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "mock-stdin": "^1.0.0",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.7",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.32.1",
    "wait-on": "8.0.3"
  },
  "build": {
    "asar": true,
    "executableName": "NextJSElectronJ",
    "appId": "com.saybackend.nextjs-electronJ",
    "asarUnpack": [
      "node_modules/next",
      "node_modules/@img",
      "node_modules/sharp",
      "**/*.node",
      "**/*.dll"
    ],
    "files": [
      "build",
      {
        "from": ".next/standalone",
        "to": "app",
        "filter": [
          "!**/.env",
          "!**/package.json"
        ]
      },
      {
        "from": ".next/static",
        "to": "app/.next/static"
      },
      {
        "from": "public",
        "to": "app/public"
      }
    ],
    "extraResources": [
      {
        "from": "front-back-server/front-back-server.exe",
        "to": "front-back-server.exe"
      }
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "copyright": "Copyright (c) 2025 Venty"
  }
}